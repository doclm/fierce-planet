    <header>
        <!--Heading-->
    </header>

    <div id="left-pane">
      <p></p>
    </div>
    <div id="content-pane">
      <div id="main-panel">
        <div id="global-info-panel">
          <div style="width: 50%; float: left">
            Level: <span id="level-display">1</span>
          </div>
          <div style="width: 50%; float: left">
            Score: <span id="score-display">0</span>
          </div>
        </div>
        <div id="level-info-panel">
          <div style="width: 25%;; float: left">
            Saved: <span id="saved-display">0</span>
          </div>
          <div style="width: 25%;; float: left">
            Expired: <span id="dead-display">0</span>
          </div>
          <div style="width: 25%;; float: left">
            Waves: <span id="waves-display">1 out of 30</span>
          </div>
          <div style="width: 25%;; float: left">
            Goodness: <span id="goodness-display">100</span>
          </div>
        </div>
        <div id="gameworld">
          <div id="world">
            <canvas id="c1" width="401" height="401" style="z-index: 1;">A Canvas</canvas>
            <canvas id="c2" width="401" height="401" style="z-index: 2;">A Canvas</canvas>
            <canvas id="c3" width="401" height="401" style="z-index: 3;">A Canvas</canvas>
          </div>

            <div id="swatch">
              <ul>
                <li><a href="#" id="eco">Economic Good <br/><small>(Cost: 10; PAY: 20)</small></a></li>
                <li><a href="#" id="env">Environmental Good <br/><small>(Cost: 10; PAY: 10)</small></a></li>
                <li><a href="#" id="soc">Social Good <br/><small>(Cost: 10; PAY: 5)</small></a></li>
              </ul>
              <div id="delete-upgrade" style="display: none;">
                  <p><a href="#" id="del" onclick="deleteCurrentPatch();">Delete <br/><small>(Revenue: 5)</small></a></p>
                  <p><a href="#" id="upg" onclick="upgradeCurrentPatch();">Upgrade <br/><small>(Cost: 20; PAY: 20)</small></a></p>
                  <p><a href="#" id="cancel" onclick="document.getElementById('delete-upgrade').style.display = 'none'">Cancel</a></p>
              </div>
              <%= javascript_include_tag "agents/world_canvas_handler" %>
            </div>

        </div>
        <div id="controls">
          <br/>
          Number of agents: <input id="numAgentsInput" name="numAgents" value="1"/>
          <br/>
          Level: <input id="levelInput" name="interval" value="1"/>
          <br/>
          Difficulty:
            <select id="difficultyInput" name="difficultyInput">
              <option value="1">Easy</option>
              <option value="2" selected="selected">Medium</option>
              <option value="3">Hard</option>
              <option value="4">EXTREME</option>
            </select>
          <br/>
          Speed: [Slow] <input id="intervalInput" name="intervalInput" type="range" min="10" max="100" value="50" step="10" onchange="resetSpeed()" /> [Fast]
          <br/>

          <button id="startAgents" onclick="startAgents();">Start</button>
          <button id="stopAgents" onclick="stopAgents();">Stop</button>
          <button id="resetWorld" onclick="resetWorld();">Restart</button>
          <button id="fullResetWorld" onclick="fullResetWorld();">Reset</button>
          <script type="text/javascript">
              drawGrid();
              drawTiles();
              drawPatches();
          </script>
          <button id="cells" onclick="drawAgents();">Debug</button>
        </div>

      </div>

    </div>
    <div id="right-pane">
      <p></p>
    </div>
